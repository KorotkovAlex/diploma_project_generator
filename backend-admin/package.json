    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.1.2",
    "@nestjs/swagger": "^4.5.9",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "archiver": "^4.0.1",
    "aws-sdk": "^2.814.0",
    "bcrypt": "^4.0.1",
    "bull": "^3.13.0",
    "bull-arena": "^2.8.1",
    "bull-board": "^0.8.0",
    "cache-manager": "^3.3.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "copy-dir": "^1.3.0",
    "into-stream": "^5.1.1",
    "nanoid": "^3.1.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.22",
    "unzipper": "^0.10.11",
    "zip-a-folder": "^0.0.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.2.0",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.1.2",
    "@types/bull": "^3.12.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.9",
    "@types/supertest": "^2.0.9",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
